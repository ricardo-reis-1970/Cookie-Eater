{
  "name": "Cookie Eater",
  "description": "Cleans Chrome's cookies for current domain",
  "version": "1.0.0",
  "author": "Ricardo Reis",
  "permissions": [
    "cookies",
    "activeTab",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "cookie-512.png",
      "24": "cookie-512.png",
      "32": "cookie-512.png" 
    },
    "default_title": "Cookie Eater"
  },
  "manifest_version": 2
}